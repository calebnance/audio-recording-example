{
  "expo": {
    "name": "expo-audio-recording",
    "description": "example of audio recording within expo",
    "scheme": "expo-audio-recording",
    "slug": "expo-audio-recording",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "githubUrl": "https://github.com/expo/audio-recording-example",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#cccccc",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "packagerOpts": {
      "assetExts": ["mp3", "mp4", "ttf"]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
